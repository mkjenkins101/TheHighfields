<!DOCTYPE html>
<html>
<body>
<h1>Home</h1>
<div id="highfield-carousel" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#highfield-carousel" data-slide-to="0" class="active"></li>
    <li data-target="#highfield-carousel" data-slide-to="1"></li>
    <li data-target="#highfield-carousel" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div id="carousel-inner" class="carousel-inner" role="listbox">
    <div class="item active">
      <%= image_tag("NK.jpg") %>
    </div>
    <div class="item">
      <%= image_tag("NK2.jpg") %>
    </div>
    <div class="item">
      <%= image_tag("NK4.jpg") %>
    </div>
  </div>

  <!-- Controls -->
  <a class="left carousel-control" href="#highfield-carousel" role="button" data-slide="prev"></a>
  <a class="right carousel-control" href="#highfield-carousel" role="button" data-slide="next"></a>
</div>
</body>
</html>
